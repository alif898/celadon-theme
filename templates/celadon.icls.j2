<scheme name="{{ metadata.name }}" version="1" parent_scheme="Darcula">
    <colors>
        <option name="CARET_ROW_COLOR" value="{{ theme.base1 }}" />
        <option name="GUTTER_BACKGROUND" value="{{ theme.base0 }}" />
        <option name="WHITESPACES" value="{{ theme.base2 }}" />

        <option name="CONSOLE_NORMAL_OUTPUT" baseAttributes="TEXT" />
        <option name="CONSOLE_SYSTEM_OUTPUT" baseAttributes="TEXT" />
        <option name="CONSOLE_ERROR_OUTPUT"><value><option name="FOREGROUND" value="{{ theme.red }}" /></value></option>
        <option name="CONSOLE_USER_INPUT"><value><option name="FOREGROUND" value="{{ theme.green }}" /><option name="FONT_TYPE" value="2" /></value></option>
        <option name="CONSOLE_BACKGROUND_KEY" value="{{ theme.base1 }}" />

        <option name="CONSOLE_BLACK"><value><option name="FOREGROUND" value="{{ theme.base0 }}" /></value></option>
        <option name="CONSOLE_RED"><value><option name="FOREGROUND" value="{{ theme.red }}" /></value></option>
        <option name="CONSOLE_GREEN"><value><option name="FOREGROUND" value="{{ theme.green }}" /></value></option>
        <option name="CONSOLE_YELLOW"><value><option name="FOREGROUND" value="{{ theme.yellow }}" /></value></option>
        <option name="CONSOLE_BLUE"><value><option name="FOREGROUND" value="{{ theme.blue }}" /></value></option>
        <option name="CONSOLE_MAGENTA"><value><option name="FOREGROUND" value="{{ theme.purple }}" /></value></option>
        <option name="CONSOLE_CYAN"><value><option name="FOREGROUND" value="{{ theme.cyan }}" /></value></option>
        <option name="CONSOLE_WHITE"><value><option name="FOREGROUND" value="{{ theme.base7 }}" /></value></option>

        <option name="FILESTATUS_ADDED" value="{{ theme.cyan }}" />
        <option name="FILESTATUS_COPIED" value="{{ theme.cyan }}" />
        <option name="FILESTATUS_DELETED" value="{{ theme.red }}" />
        <option name="FILESTATUS_IDEA_FILESTATUS_DELETED_FROM_FILE_SYSTEM" value="{{ theme.red }}" />
        <option name="FILESTATUS_IDEA_FILESTATUS_IGNORED" value="{{ theme.bone_alt }}" />
        <option name="FILESTATUS_IDEA_FILESTATUS_MERGED_WITH_BOTH_CONFLICTS" value="{{ theme.red }}" />
        <option name="FILESTATUS_IDEA_FILESTATUS_MERGED_WITH_CONFLICTS" value="{{ theme.red }}" />
        <option name="FILESTATUS_IDEA_FILESTATUS_MERGED_WITH_PROPERTY_CONFLICTS" value="{{ theme.orange }}" />
        <option name="FILESTATUS_MERGED" value="{{ theme.purple_alt }}" />
        <option name="FILESTATUS_MODIFIED" value="{{ theme.purple }}" />
        <option name="FILESTATUS_NOT_CHANGED_IMMEDIATE" value="{{ theme.base4 }}" />
        <option name="FILESTATUS_NOT_CHANGED_RECURSIVE" value="{{ theme.base4 }}" />
        <option name="FILESTATUS_RENAMED" value="{{ theme.blue }}" />
        <option name="FILESTATUS_UNKNOWN" value="{{ theme.red_alt }}" />
        <option name="FILESTATUS_addedOutside" value="{{ theme.cyan_mod1 }}" />
        <option name="FILESTATUS_changelistConflict" value="{{ theme.red }}" />
        <option name="FILESTATUS_modifiedOutside" value="{{ theme.purple_mod1 }}" />
    </colors>
    <attributes>
        <option name="TEXT">
            <value>
                <option name="FOREGROUND" value="{{ theme.base4 }}" />
                <option name="BACKGROUND" value="{{ theme.base0 }}" />
            </value>
        </option>

        <option name="DEFAULT_KEYWORD"><value><option name="FOREGROUND" value="{{ theme.pink }}" /><option name="FONT_TYPE" value="1" /></value></option>
        <option name="DEFAULT_STRING"><value><option name="FOREGROUND" value="{{ theme.purple }}" /></value></option>
        <option name="DEFAULT_NUMBER"><value><option name="FOREGROUND" value="{{ theme.yellow }}" /></value></option>

        <option name="DEFAULT_FUNCTION_CALL"><value><option name="FOREGROUND" value="{{ theme.green }}" /></value></option>
        <option name="DEFAULT_FUNCTION_DECLARATION"><value><option name="FOREGROUND" value="{{ theme.green }}" /></value></option>

        <option name="DEFAULT_CLASS_NAME"><value><option name="FOREGROUND" value="{{ theme.cyan }}" /></value></option>
        <option name="DEFAULT_CLASS_REFERENCE"><value><option name="FOREGROUND" value="{{ theme.blue_alt }}" /></value></option>
        <option name="DEFAULT_INTERFACE_NAME"><value><option name="FOREGROUND" value="{{ theme.blue }}" /></value></option>
        <option name="DEFAULT_CONSTANT"><value><option name="FOREGROUND" value="{{ theme.orange }}" /><option name="FONT_TYPE" value="2" /></value></option>
        <option name="DEFAULT_PREDEFINED_SYMBOL"><value><option name="FOREGROUND" value="{{ theme.pink }}" /></value></option>
        <option name="DEFAULT_STATIC_FIELD"><value><option name="FOREGROUND" value="{{ theme.blue_alt }}" /><option name="FONT_TYPE" value="2" /></value></option>
        <option name="DEFAULT_STATIC_METHOD"><value><option name="FOREGROUND" value="{{ theme.green }}" /><option name="FONT_TYPE" value="2" /></value></option>
        <option name="DEFAULT_INSTANCE_FIELD"><value><option name="FOREGROUND" value="{{ theme.blue_alt }}" /></value></option>
        <option name="DEFAULT_ATTRIBUTE"><value><option name="FOREGROUND" value="{{ theme.orange }}" /></value></option>
        <option name="DEFAULT_LOCAL_VARIABLE"><value><option name="FOREGROUND" value="{{ theme.base4 }}" /></value></option>

        <option name="DEFAULT_METADATA"><value><option name="FOREGROUND" value="{{ theme.lime }}" /></value></option>
        <option name="DEFAULT_IDENTIFIER"><value><option name="FOREGROUND" value="{{ theme.base4 }}" /></value></option>
        <option name="DEFAULT_ENTITY"><value><option name="FOREGROUND" value="{{ theme.yellow }}" /></value></option>

        <option name="DEFAULT_COMMA"><value /></option>
        <option name="DEFAULT_SEMICOLON"><value /></option>

        <option name="DEFAULT_LINE_COMMENT"><value><option name="FOREGROUND" value="{{ theme.bone }}" /></value></option>
        <option name="DEFAULT_BLOCK_COMMENT"><value><option name="FOREGROUND" value="{{ theme.bone }}" /></value></option>
        <option name="DEFAULT_DOC_COMMENT"><value><option name="FOREGROUND" value="{{ theme.bone }}" /><option name="FONT_TYPE" value="2" /></value></option>
        <option name="DEFAULT_DOC_COMMENT_TAG"><value><option name="FOREGROUND" value="{{ theme.cream }}" /><option name="EFFECT_TYPE" value="1" /></value></option>
        <option name="DEFAULT_DOC_COMMENT_TAG_VALUE"><value><option name="FOREGROUND" value="{{ theme.orange_alt }}" /></value></option>

        <option name="DEFAULT_VALID_STRING_ESCAPE"><value><option name="FOREGROUND" value="{{ theme.base4 }}" /></value></option>
        <option name="FOLDED_TEXT_ATTRIBUTES"><value><option name="FOREGROUND" value="{{ theme.cream }}" /><option name="BACKGROUND" value="{{ theme.base1 }}" /></value></option>
        <option name="NOT_USED_ELEMENT_ATTRIBUTES"><value><option name="FOREGROUND" value="{{ theme.cream }}" /></value></option>

        <option name="BAD_CHARACTER"><value><option name="EFFECT_COLOR" value="{{ theme.red }}" /><option name="EFFECT_TYPE" value="4" /></value></option>
        <option name="DEFAULT_INVALID_STRING_ESCAPE"><value><option name="FOREGROUND" value="{{ theme.red_alt }}" /><option name="EFFECT_COLOR" value="{{ theme.red }}" /><option name="EFFECT_TYPE" value="4" /></value></option>
        <option name="ERRORS_ATTRIBUTES"><value><option name="EFFECT_COLOR" value="{{ theme.red }}" /><option name="ERROR_STRIPE_COLOR" value="{{ theme.red }}" /><option name="EFFECT_TYPE" value="4" /></value></option>
        <option name="WARNING_ATTRIBUTES"><value><option name="EFFECT_COLOR" value="{{ theme.orange }}" /><option name="ERROR_STRIPE_COLOR" value="{{ theme.orange }}" /><option name="EFFECT_TYPE" value="2" /></value></option>

        <option name="ANNOTATION_ATTRIBUTE_NAME_ATTRIBUTES" baseAttributes="DEFAULT_METADATA" />
        <option name="ANNOTATION_NAME_ATTRIBUTES" baseAttributes="DEFAULT_METADATA" />

        <option name="XML_PROLOGUE" baseAttributes="TEXT" />
        <option name="XML_TAG" baseAttributes="DEFAULT_IDENTIFIER" />
        <option name="XML_TAG_NAME" baseAttributes="DEFAULT_IDENTIFIER" />
        <option name="XML_ATTRIBUTE_NAME" baseAttributes="DEFAULT_ATTRIBUTE" />
        <option name="XML_ENTITY_REFERENCE" baseAttributes="DEFAULT_ENTITY" />
        <option name="XML_TAG_DATA" baseAttributes="TEXT" />

        <option name="IDENTIFIER_UNDER_CARET_ATTRIBUTES"><value><option name="BACKGROUND" value="{{ theme.accent_alt2 }}" /><option name="ERROR_STRIPE_COLOR" value="{{ theme.accent }}" /><option name="EFFECT_TYPE" value="1" /></value></option>
        <option name="INJECTED_LANGUAGE_FRAGMENT"><value><option name="BACKGROUND" value="{{ theme.cream_alt2 }}" /></value></option>
        <option name="MATCHED_BRACE_ATTRIBUTES"><value><option name="BACKGROUND" value="{{ theme.base2 }}" /><option name="FONT_TYPE" value="1" /><option name="EFFECT_COLOR" value="{{ theme.accent }}" /><option name="EFFECT_TYPE" value="1" /></value></option>
        <option name="TEXT_SEARCH_RESULT_ATTRIBUTES"><value><option name="BACKGROUND" value="{{ theme.accent_alt2 }}" /><option name="EFFECT_COLOR" value="{{ theme.accent_alt }}" /><option name="ERROR_STRIPE_COLOR" value="{{ theme.accent }}" /></value></option>
        <option name="SEARCH_RESULT_ATTRIBUTES"><value><option name="BACKGROUND" value="{{ theme.accent_alt2 }}" /><option name="ERROR_STRIPE_COLOR" value="{{ theme.accent }}" /></value></option>
        <option name="WRITE_IDENTIFIER_UNDER_CARET_ATTRIBUTES"><value><option name="BACKGROUND" value="{{ theme.accent_alt }}" /><option name="ERROR_STRIPE_COLOR" value="{{ theme.accent }}" /><option name="EFFECT_TYPE" value="1" /></value></option>
        <option name="WRITE_SEARCH_RESULT_ATTRIBUTES"><value><option name="BACKGROUND" value="{{ theme.accent_alt }}" /><option name="ERROR_STRIPE_COLOR" value="{{ theme.accent }}" /></value></option>

    </attributes>
</scheme>